apiVersion: v1
clusters:
- cluster:
    certificate-authority: /etc/kubernetes/ssl/ca.pem
    server: https://${MASTER_HOST}:6443
  name: ${K8S_CLUSTER_ID}
contexts:
- context:
    cluster: ${K8S_CLUSTER_ID}
    namespace: kube-system
    user: admin@${K8S_CLUSTER_ID}
  name: ${K8S_CLUSTER_ID}_admin_kube-system
- context:
    cluster: ${K8S_CLUSTER_ID}
    namespace: default
    user: admin@${K8S_CLUSTER_ID}
  name: ${K8S_CLUSTER_ID}_admin_default
current-context: ${K8S_CLUSTER_ID}_admin_kube-system
kind: Config
preferences: {}
users:
- name: admin@${K8S_CLUSTER_ID}
  user:
    client-certificate: /etc/kubernetes/ssl/admin.pem
    client-key: /etc/kubernetes/ssl/admin-key.pem
    username: admin
